{
  "name": "parsegraph-normalizewheel",
  "version": "1.4.4-dev",
  "description": "A fix found for normalizing wheel input",
  "main": "dist/parsegraph-normalizewheel.js",
  "types": "dist/parsegraph-normalizewheel.d.ts",
  "scripts": {
    "coverage": "nyc npm run test",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha",
    "autotest": "cross-env TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha --watch",
    "demo": "node demo/demo.js",
    "autobuild": "webpack --watch --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "build-prod": "NODE_ENV=production webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/parsegraph/normalizewheel.git"
  },
  "author": "Rainback, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parsegraph/normalizewheel/issues"
  },
  "homepage": "https://github.com/parsegraph/normalizewheel#readme",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-babel",
    "reporter": [
      "html"
    ],
    "exclude": [
      "test/**.js",
      "dist/**.js"
    ],
    "reportDir": "dist/coverage"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.12.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.0.0",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "prettier": "2.1.2",
    "raw-loader": "^4.0.1",
    "source-map-loader": "^1.0.2",
    "source-map-support": "^0.5.19",
    "ts-loader": "^9.2.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.2",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "babel-jest": "^27.5.1",
    "chai": "^4.3.6",
    "jest": "^27.5.1",
    "typedoc": "^0.22.12"
  }
}
